<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://appengine.google.com/ns/1.0">
  <!-- Deployment settings -->
  <application>vmruntime-demo</application>
  <version>jetty9-vm-annotations-perf-test</version>

  <!-- AppEngine VM Runtime Settings -->
  <vm>true</vm>
  <manual-scaling>
    <instances>1</instances>
  </manual-scaling>

  <sessions-enabled>true</sessions-enabled>
  <precompilation-enabled>false</precompilation-enabled>
  <threadsafe>true</threadsafe>

  <!--static-files>
      <include path="**"/>
  </static-files-->

  <resource-files />

</appengine-web-app>